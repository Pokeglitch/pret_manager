TagGUI[which=silver] {
    background-color: #8aa0a6;
}

TagGUI[which=silver]:hover {
    background-color: #9fb1b6;
}

TagGUI[which=crystal] {
    background-color: #50bde6;
}

TagGUI[which=crystal]:hover {
    background-color: #70c9ea;
}

TagGUI[which=official] {
    background-color: #ffffff;
}

TagGUI[which=official] * {
    color: #da0000;
}

TagGUI[which=official]:hover {
    background-color: #ffffff;
}

TagGUI[which=official]:hover * {
