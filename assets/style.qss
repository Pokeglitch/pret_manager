QMainWindow {
    background-color: #252526;
    color: #ffffff;
    min-height:800px;
    min-width:1400px;
}

QDialog, QDialog > QLabel {
    background-color: #252526;
    color: #ffffff;
}

QScrollArea * {
    padding: 5;
}

QWidget#status * {
    background-color: #000000;
    color: #FFFFFF;
    padding: 5;
}

QLabel#header {
    color: #ffffff;
    font-size: 22px;
    margin: 5;
}

QWidget#header-frame, QWidget#footer {
    border: 1px solid black;
    min-height:50px;
    max-height:50px;
}

QWidget#header-frame {
    border-left: none;
}

QScrollBar {
    background-color: #252526;
    border-radius:3px;
}

QAbstractScrollArea {
    background-color: #252526
}

QScrollBar::handle {
    background: white;
    border:1px solid black;
    border-radius:3px;
}

QScrollBar::handle:horizontal {
    min-width: 20px;
}

QScrollBar::handle:vertical {
    min-height: 20px;
}

QScrollBar::add-line, QScrollBar::sub-line {
    border: none;
    background: none;
}

QScrollBar::add-page, QScrollBar::sub-page {
    border: none;
    background: #121213;
    border-radius:3px;
}

QLineEdit {
    height: 18px;
    font-size:15px;
    padding: 5px;
    color:white;
    border-radius:7px;
    border: 1px solid black;
    background-color: #3a3a3b
}

CatalogsModeIcon {
    border: 1px solid #2b2b2b;
    border-radius: 5px;
    min-height: 30px;
    max-height: 30px;
    min-width: 30px;
    max-width: 30px;
    background-color: #2b2b2b;
    margin: 5px;
}

CatalogsModeIcon[mode=Or]{
    background-color: #274e13;
}

CatalogsModeIcon[mode=Or]:hover{
    background-color: #315c1b;
}

CatalogsModeIcon[mode=Or][active=true]{
    background-color: #315c1b;
}

CatalogsModeIcon[mode=And]{
    background-color: #725600;
}

CatalogsModeIcon[mode=And]:hover{
    background-color: #8b6f19;
}

CatalogsModeIcon[mode=And][active=true]{
    background-color: #8b6f19;
}

CatalogsModeIcon[mode=Not], *[bg=red] {
    background-color: #7b3131;
}

CatalogsModeIcon[mode=Not]:hover, *[bg=red]:hover {
    background-color: #9f3f3f;
}

CatalogsModeIcon[mode=Not][active=true]{
    background-color: #9f3f3f;
}

CatalogsModeIcon[active=true] {
    border: 1px solid #ffffff;
}

CatalogsTabs {
    background-color: #2b2b2b;
}

CatalogsHeader * {
    margin: 10px;
}

CatalogsHeader > QLabel {
    color: #ffffff;
    font-size:22px;
}

CatalogsTabs CatalogIcon {
    border: 1px solid black;
    border-top: none;
    min-height: 50px;
    max-height: 50px;
    background-color: #313131;
}

CatalogsTabs CatalogIcon:hover {
    background-color: #3a3a3b;
}

CatalogsTabs CatalogIcon[active=true] {
    background-color: #252526;
    border-right: none;
}

Queue VScroll, CatalogGUI VScroll {
    background-color: #252526;
}

Queue QScrollArea, CatalogGUI VScroll, CatalogGUI QScrollArea {
    border: none;
}

Queue QScrollArea, #QueueFooter {
    border-right: 1px solid black;
}

PanelHeader, PanelHeader QLabel, QueueHeader, QueueHeader QLabel {
    background-color: #252526;
    color: #ffffff;
}

Queue Button {
    font-size: 12px;
    min-width: 200px;
    max-width: 200px;
    background-color: #0E639C;
    color: #EEEEEE;
}

Queue Button:hover {
    background-color: #1176BA;
    color: #ffffff;
}

Queue Button[disabled=true], Queue Button[disabled=true]:hover, Queue Button[processing=true], Queue Button[processing=true]:hover {
    background-color: #094166;
    color: #bbbbbb
}

Button:hover {
    background-color: #1176BA;
    color: #ffffff;
}

Button {
    border: 1px solid black;
    border-radius: 10px;
    max-height: 30px;
    min-height: 30px;
    min-width: 60px;
    margin: 5;
    background-color: #0E639C;
    color: #EEEEEE;
}

Button[disabled=true], Button[disabled=true]:hover, Button[processing=true], Button[processing=true]:hover {
    background-color: #094166;
    color: #bbbbbb
}

CatalogEntryGUI[mode="Not"] {
    background-color: #7b3131
}

CatalogEntryGUI[mode="Not"]:hover {
    background-color: #9f3f3f
}

CatalogEntryGUI[mode="And"] {
    background-color: #725600
}

CatalogEntryGUI[mode="And"]:hover {
    background-color: #8b6f19
}

GameQueue, CatalogEntryGUI {
    min-height: 18px;
    max-height: 18px;
    background-color: #252526;
}

FlagListEntryGUI {
    min-height: 40px;
    max-height: 40px;
}

GameQueue *, CatalogEntryGUI * {
    font-size: 12px;
    color: white;
}

GameQueue:hover, CatalogEntryGUI:hover, GameTile:hover {
    background-color: #3a3a3b
}

GameQueue[active="true"], CatalogEntryGUI[mode="New"] {
    background-color: #4C4C54;
}

GameQueue[active="true"]:hover, CatalogEntryGUI[mode="New"]:hover {
    background-color: #4C4C54;
}

CatalogEntryGUI[mode="Or"], *[bg=green] {
    background-color: #274e13;
}

CatalogEntryGUI[mode="Or"]:hover, *[bg=green]:hover {
    background-color: #315c1b;
}

GameTile[queued="true"] {
    border: 2px solid #000000;
    min-width:130px;
    max-width:130px;
    min-height:150px;
    max-height:150px;
    background-color: #464646;
}

GameTile[queued="true"]:hover {
    background-color: #5a5a5a;
}

GameTile[active="true"] {
    min-width:132px;
    max-width: 132px;
    min-height: 152px;
    max-height: 152px;
    border: 1px solid #000000;
    background-color: #555555;
}

GameTile[active="true"]:hover {
    background-color: #606060;
}

GameQueue[processing="true"], CatalogEntryGUI[processing="true"], GameTile[processing="true"] {
    background-color: #1b5780;
}

GameQueue[processing="true"]:hover, CatalogEntryGUI[processing="true"]:hover, GameTile[processing="true"]:hover {
    background-color: #226592;
}

GameTile {
    min-width:134px;
    max-width: 134px;
    min-height: 154px;
    max-height: 154px;
    border-radius:10px;
}

GameTile * {
    color: #ffffff;
}

LabelImage {
    margin-top:38px;
    border: 1px solid black;
    padding: 0px;
    border-radius:5px;
}

GameTileTitle {
    padding: 0px;
    margin-top:18px;
    min-height:24px;
    max-height:24px;
    min-width:100px;
    max-width: 100px;
    border-radius: 12px;
    color: black;
    border: 2px solid black;
}

GameTileTitle[excluding=true] {
    border-color: rgba(0, 0, 0, 0.5)
}

GameTileTitle[library=false]{
    background-color: rgba(255, 255, 255, 0.08)
}

GameTileTitle[library=true]{
    background-color: rgba(200, 200, 200, 0.08)
}

#TileArt {
    margin: 0px;
    padding: 0px;
    min-width: 100px;
    max-width: 100px;
    min-height: 100px;
    max-height: 100px;
    background-color: #ffffff;
}

QWidget#Tiles {
    background-color: #252526;
    border: none;
    margin-left:5;
    margin-top:5;
}

QWidget#Tag {
    min-height: 20px;
    max-height: 20px;
    min-width: 64px;
    max-width: 64px;
    border-radius: 8px;
    border: 1px solid black;
    padding: 0px;
    margin:2px;
}

TagGUI {
    background-color: #ffffff;
}

TagGUI * {
    color: #000000;
    font-size: 11px;
}

TagGUI[which=red] {
    background-color: #e43232;
}

TagGUI[which=red]:hover {
    background-color: #e64646;
}

TagGUI[which=green] {
    background-color: #1aba00;
}

TagGUI[which=green]:hover {
    background-color: #1dcf00;
}

TagGUI[which=blue] {
    background-color: #275abf;
}

TagGUI[which=blue]:hover {
    background-color: #2c65d5;
}

TagGUI[which=yellow] {
    background-color: #e5d200;
}

TagGUI[which=yellow]:hover {
    background-color: #ffea00;
