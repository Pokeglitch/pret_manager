TagGUI[which=disasm] {
    background-color: #ffffff;
}

TagGUI[which=disasm] * {
    color: #000000;
}

TagGUI[which=disasm]:hover {
    background-color: #ffffff;
}

TagGUI[which=disasm]:hover * {
    color: #333333;
}

TagGUI[which=debug] {
    background-color: #a3e1d3;
