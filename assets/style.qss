GamePanelArtwork {
    min-height: 300px;
    max-height: 300px;
    min-width: 300px;
    max-width: 300px;
    border: 1px solid black;
    margin:5px;
    padding: 0;
}

PanelBody, GamePanel, GamePanelBody *, Field *, #Trees, #Trees *, GameDescription {
    background-color: #252526;
    color: #ffffff;
    border: none;
    font-size:12px;
}

PanelOptions * {
    color: #ffffff;
    font-size:12px;
}

PanelButton {
    min-width: 200px;
    max-width: 200px;
    padding: 0px;
}

Panel QScrollArea, Panel VScroll {
    border: none;
}

PanelBody QComboBox {
    padding: 5px;
    color:white;
    border-radius:5px;
    border: 1px solid black;
    background-color: #3a3a3b;
}

PanelBody QComboBox[processing=true] {
    color: #616161;
    background-color: #313131;
}

PanelBody QComboBox::down-arrow {
    background: none;
    border: none;
    image: url('assets/images/down_arrow.png');
}

PanelBody QComboBox::drop-down {
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
}

PanelBody QComboBox QAbstractItemView {
    outline: 0px;
    border: 1px solid black;
    selection-background-color: #3a3a3b;
}

PanelHeading {
    font-size: 20px;
    font-weight: bold;
}

EnvironmentsRow QLabel {
    margin: 5px;
    max-width:50px;
    min-width: 50px;
}

EnvironmentComboBox {
    max-width: 120px;
    min-width: 120px;
    margin: 5px;
    font-size: 12px;
}

QLabel#url {
    color: #2478b7;
    text-decoration: underline;
}

#PanelClickable {
    min-height: 20px;
    max-height: 20px;
    border-radius: 8px;
    border: 1px solid black;
    padding: 0px;
    padding-left: 8px;
    padding-right: 8px;
    margin:2px;
    background: #3a3a3b;
}

#PanelClickable:hover {
    background-color: #505050;
}

QTreeWidget {
    border: none;
}

QTreeView::branch:closed:has-children:hover {
    image: url('assets/images/arrow_filled.png');
}

QTreeView::branch:open {
    image: url('assets/images/down_arrow.png');
}

QTreeView::branch:closed:has-children {
    image: url('assets/images/arrow_hollow.png');
}

QTreeView::item {
    border: 1px solid #252526;
    border-radius: 5px;
    color: #ffffff;
}

QTreeView::item:!selected:hover {
    border: 1px solid #888888;
}

QTreeView::item:selected {
    border: 1px solid white;
}

QTreeView::item:selected:hover {
    border: 1px solid white;
    background: #333333;
}

QSlider::groove {
    background: transparent;
    border: none;
}

QSlider::handle {
    border: 1px solid #888888;
    background: #cccccc;
    height:20px;
    width:20px;
    border-radius: 10px;
}

QSlider::handle[active=true] {
    border: 1px solid #000000;
    background: #ffffff;
}


Panel ToggleBG, Panel ToggleSlider {
    padding: 0px;
    margin-right: 10px;
    margin-left: 10px;
}


ToggleBG {
    min-height: 14px;
    max-height: 14px;
    background: white;
    border: 1px solid black;
    border-radius: 7px;
    border: 1px solid #777777;
    background: #bbbbbb;
}

ToggleBG[active=true] {
    border: 1px solid black;
    background: #2a84c0;
}

Toggle {
    min-width:60px;
    max-width:60px;
}

Process QScrollBar, Process QAbstractScrollArea{
    background-color: black;
}

ProcessTitle {
    border-top: 1px solid black;
    border-bottom: 1px solid black;
    min-height:50px;
    max-height:50px;
}

ProcessTitle > QLabel {
    font-size:22px;
    color: white;
}

ProcessTitle[processing=true] {
    background-color: #1b5780;
}

ToggleField > QLabel {
    font-size:18px;
    color: white;
    margin-left: 10px;
}

ToggleBG, ToggleSlider {
    margin-right: 10px;
}

QMenu {
    border: 1px solid white;
    background-color: #252526;
    color: white;
    margin: 0px;
    padding: 0px;
}

 QMenu::item {
    background-color: transparent;
 }

 QMenu::item:hover {
    background-color: #2b2b2b;
 }

 QMenu::item:selected{
    background-color: #313131;
 }

PanelHeaderClose {
    margin-left: 10px;
    margin-right: 10px;
    padding: 10px;
    border: none;
    border-radius: 5px;
}

PanelHeaderClose:hover {
    background-color: #313131;
}

 MenuIcon > QLabel {
    padding-left: 10px;
    padding-right: 10px;
    background: none;
}
